<%= form_tag '/login' do %>
  <h2>Login</h2>
  <div class="form-group">
    <p><label for="Email" class="floatLabel">Email</label></p>
    <%= text_field_tag :email %>
  </div>
  <div class="form-group">
    <p><label for="password" class="floatLabel">Password</label></p>
    <%= password_field_tag :password %>
  </div>
  <%= submit_tag "Login", class: 'btn btn-primary' %>
<% end %>